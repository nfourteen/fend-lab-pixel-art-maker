{
  "name": "lab-pixel-maker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "David Nimorwicz",
  "license": "ISC",
  "paths": {
    "src": {
      "base": "./src/",
      "scss": "./src/sass/",
      "customBootstrap": "./src/sass/custom-bootstrap/",
      "fonts": "./src/fonts/",
      "js": "./src/js/",
      "images": "./src/images/"
    },
    "build": {
      "base": "./build/",
      "css": "./build/css/",
      "js": "./build/js/",
      "fonts": "./build/fonts/",
      "images": "./build/images/"
    },
    "dist": {
      "base": "./public/",
      "css": "./public/css/",
      "js": "./public/js/",
      "fonts": "./public/fonts/",
      "images": "./public/images/"
    }
  },
  "globs": {
    "distCss": [
      "./build/css/*.css",
      "./src/css/*.css"
    ],
    "distJs": [
      "./build/js/*.js"
    ],
    "babelJs": [
      "./src/js/*.js"
    ]
  },
  "vars": {
    "scssFileName": "styles.scss",
    "siteCssFileName": "styles.css"
  },
  "devDependencies": {
    "babel-core": "^6.26.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.23.7",
    "del": "^3.0.0",
    "fancy-log": "latest",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-cached": "^1.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-filter": "^5.1.0",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.0",
    "gulp-print": "^5.0.0",
    "gulp-sass": "latest",
    "gulp-sourcemaps": "^2.6.0",
    "yargs": "^7.1.0"
  }
}
